{
  "manifest_version": 2,

  "name": "+1 link Extension",
  "description": "Add links to add1.link.",
  "version": "0.0.2",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "+1L",
    "default_icon": "logo-small.png"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "96": "logo-small.png",
    "128": "logo-small.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
