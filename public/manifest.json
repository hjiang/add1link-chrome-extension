{
  "manifest_version": 2,

  "name": "Add-1-link Extension",
  "description": "Add links to add1.link.",
  "version": "0.0.1",

  "browser_action": {
    "default_popup": "index.html",
    "default_title": "+1L"
  },
  "icons": {
    "16": "logo-small.png",
    "48": "logo-small.png",
    "128": "logo-small.png"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}
